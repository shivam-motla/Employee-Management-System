@model IEnumerable<AddAddress>
@{
}
<h1>Data</h1>

<table class="table table-striped">
    <tr>
        <th>Index </th>
        <th>Name</th>
        <th>Address</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>


    
    @foreach(var obj in Model)
    {   <tr>
            <td>@obj.Id</td>
            <td>@obj.Name</td>
            <td>@obj.Address</td>

            <td >
                <a asp-controller="Form" asp-action="Edit" asp-route-id="@obj.Id"> Edit</a>
            </td>

            <td >
                <a asp-controller="Form" asp-action="Delete" asp-route-id="@obj.Id"> Delete</a>
            </td>

        </tr>



    }
    

</table>